{
    "extends": [
        "next",
        "next/core-web-vitals",
        "prettier"
    ],
    "plugins": [
        "next",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "warn",{
                "arrowParens": "always",
                "singleQuote": true,
                "bracketSameLine": false,
                "bracketSpacing": true,
                "embeddedLanguageFormatting": "auto",
                "htmlWhitespaceSensitivity": "css",
                "insertPragma": false,
                "jsxSingleQuote": false,
                "printWidth": 120,
                "proseWrap": "preserve",
                "quoteProps": "as-needed",
                "requirePragma": false,
                "semi": true,
                "tabWidth": 4,
                "trailingComma": "all",
                "useTabs": false,
                "vueIndentScriptAndStyle": false,
                "arrowParents": "avoid"
            }
            
        ]
    }
}